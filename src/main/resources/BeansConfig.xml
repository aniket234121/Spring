<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- ALL Beans -->

    <!--Setter Injection through XML-->
    <bean class="springCore.Beans.Student" name="student1">
        <property name="id">
            <value>1</value>
        </property>
        <property name="name">
            <value>Aniket Chauhan</value>
        </property>
        <property name="age">
            <value>12</value>
        </property>
    </bean>



</beans>